{
  "manifest_version": 3,
  "name": "Excalisave",
  "version": "2.3.0",
  "short_name": "Excalisave",
  "description": "Save your excalidraw drawings.",
  "__chrome__key": "",
  "__chrome__oauth2": {
    "client_id": "651009965666-kco77ep3340oodhkimnis2id1g004nvi.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/drive.file"
    ]
  },
  "icons": {
    "16": "assets/icons/16.png",
    "48": "assets/icons/48.png",
    "128": "assets/icons/128.png"
  },
  "homepage_url": "https://github.com/atharvakadlag/excalisave",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "scripting",
    "unlimitedStorage",
    "identity",
    "identity.email"
  ],
  "__chrome|firefox__author": "atharvakadlag",
  "__opera__developer": {
    "name": "atharvakadlag"
  },
  "__firefox|prod__browser_specific_settings": {
    "gecko": {
      "id": "excalisave@atharvakadlag.github.io"
    }
  },
  "__chrome__minimum_chrome_version": "88",
  "__opera__minimum_opera_version": "36",
  "action": {
    "default_title": "Excalisave",
    "default_popup": "popup.html"
  },
  "__chrome|opera__options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      }
    }
  },
  "__chrome|opera__background": {
    "service_worker": "js/background.bundle.js"
  },
  "__firefox__background": {
    "scripts": [
      "js/background.bundle.js"
    ]
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*"
      ],
      "matches": [
        "https://excalidraw.com/*"
      ]
    }
  ],
  "host_permissions": [
    "https://excalidraw.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://excalidraw.com/*"
      ],
      "js": [
        "libs/react.production.min.js",
        "libs/react-dom.production.min.js",
        "libs/excalidraw.production.min.js",
        "js/content-scripts/listenDrawingUpdates.bundle.js"
      ]
    },
    {
      "matches": [
        "https://excalidraw.com/*"
      ],
      "js": [
        "js/content-scripts/addOverwriteAction.bundle.js"
      ],
      "run_at": "document_idle"
    }
  ]
}